<div class="tbl-object">
  <%= image_tag @organization.logo_url, class: "avatar tbl-item" %>
  <div class="tbl-item tbl-item-primary">
    <h1 class="org-name">
      <%= @organization.name %>
    </h1>
  </div>
</div>

<div class="tabs">
