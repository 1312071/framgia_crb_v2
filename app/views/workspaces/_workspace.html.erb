<div class="media">
  <div class="media-left">
    <a href="#">
      <%= image_tag workspace.logo_url, class:"media-object",
        size: Settings.workspace.logo_size %>
    </a>
  </div>
  <div class="media-body">
    <h4 class="media-heading"><%= workspace.name %></h4>
    <i class="fa fa-map-marker" aria-hidden="true"></i> <%= workspace.address %>
  </div>
</div>
