<div id="popup" class="dialog dialog-hidden">
  <div class="bubblemain">
    <div id="bubble-close" class="bubble-close close-popup cancel-popup-event"></div>
    <div id="bubbleContent" class="bubblecontent">
      <div class="tab-container">
        <div id="current-user-id-popup" ><%= current_user.id if user_signed_in? %></div>
        <div id="head-popup">
          <div class="title-popup-wap" >
            <div id="color-event"></div>
            <input id="title-input-popup" class="title-popup data-none-display">
            <div id="title-popup" class="title-popup data-display">
              <%= event.title %>
            </div>
          </div>
        </div>
        <div class="form-group">
          <label><%= t ".place" %></label>
          <%= event.name_place || event.place.name %>
        </div>
        <div id="data-popup" class="form-group">
          <div id="time-data">
            <label><%= t ".time" %></label>
            <span id="time-event-popup" class="tile-content">
              <%= event.all_day? ? "#{start_date}" : "#{start_date} To #{finish_date}" %>
            </span>
          </div>
          <div id="calendar-data">
            <label><%= t ".calendar" %></label>
            <span id="calendar-event-popup" class="tile-content">
              <%= event.calendar_name %>
            </span>
          </div>
        </div>
        <div id="footer-popup">
          <% if params[:controller] == "api/particular_events" %>
            <%= link_via_permission event, fdata %>
          <% else %>
            <%= btn_via_permission user, event, fdata %>
          <% end %>
        </div>
      </div>
    </div>
    <div id="prong-popup" class="bottom-prong">
      <div class="prong-dk"></div>
      <div class="prong-lt"></div>
    </div>
  </div>
</div>
