<%= form_for(@place) do |f| %>
  <%= render "shared/errors_messages", object: @place %>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :address %>
    <%= f.text_field :address, class: "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit :submit, class: "btn button" %>
  </div>
<% end %>
